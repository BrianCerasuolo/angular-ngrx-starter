{
  "name": "angular-ngrx-starter",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e": "ng e2e",
    "prod": "ng build --prod --e=prod --no-sourcemap",
    "lint:check": "ng lint --type-check",
    "lint:fix": "yarn-or-npm run lint:check -- --fix",
    "check": "yarn-or-npm run lint:check && yarn-or-npm run prettier:check",
    "check:fix": "yarn-or-npm run lint:fix && yarn-or-npm run prettier:fix",
    "prettier:base": "yarn-or-npm run prettier -- --single-quote --trailing-comma es5",
    "prettier:base-files": "yarn-or-npm run prettier:base -- './{e2e,src}/**/*.{scss,ts}'",
    "prettier:fix": "yarn-or-npm run prettier:base-files -- --write",
    "prettier:check": "yarn-or-npm run prettier:base-files -- -l",
    "precommit": "lint-staged",
    "prepush": "yarn-or-npm run lint:check"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,scss}": [
        "yarn-or-npm run prettier:base -- -l"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.1",
    "@angular/common": "4.3.1",
    "@angular/compiler": "4.3.1",
    "@angular/core": "4.3.1",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "4.3.1",
    "@angular/http": "4.3.1",
    "@angular/material": "2.0.0-beta.6",
    "@angular/platform-browser": "4.3.1",
    "@angular/platform-browser-dynamic": "4.3.1",
    "@angular/router": "4.3.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.3",
    "@ngrx/store": "2.2.2",
    "@ngrx/store-devtools": "3.2.4",
    "angular2-prettyjson": "2.0.5",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "material-design-icons-iconfont": "3.0.3",
    "ng2-translate": "5.0.0",
    "ngrx-store-freeze": "0.1.9",
    "redux": "3.7.2",
    "redux-batched-actions": "0.2.0",
    "rxjs": "5.4.2",
    "tslint-config-prettier": "1.1.0",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.2.3",
    "@angular/compiler-cli": "4.3.1",
    "@types/jasmine": "2.5.53",
    "@types/jest": "20.0.4",
    "@types/node": "8.0.14",
    "codelyzer": "3.1.2",
    "husky": "0.14.3",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "jest": "20.0.4",
    "jest-preset-angular": "2.0.5",
    "lint-staged": "4.0.2",
    "prettier": "1.5.3",
    "protractor": "5.1.2",
    "ts-node": "3.2.1",
    "tslint": "5.5.0",
    "typescript": "2.3.4",
    "yarn-or-npm": "2.0.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-jest.ts",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/environment/**",
      "!src/**/*.mock.ts",
      "!src/mocks/**",
      "!src/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/environment",
      "<rootDir>/src/mocks"
    ]
  }
}
